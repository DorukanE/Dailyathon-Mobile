            android:id="@+id/action_destination_profile_to_fragmentUpdateProfile"
            app:destination="@id/fragmentUpdateProfile"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
