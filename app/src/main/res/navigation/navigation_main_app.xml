            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/fragmentEditTags"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.fragmentsProfile.FragmentEditTags"
        android:label="fragment_edit_tags"
        tools:layout="@layout/fragment_edit_tags" >
        <action
