            android:id="@+id/action_destination_home_self"
            app:destination="@id/destination_home"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
    </fragment>
    <fragment
        android:id="@+id/destination_news"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.FragmentNews"
        android:label="fragment_news"
