            android:id="@+id/action_destination_news_to_fragmentPersonalNewsDetail"
            app:destination="@id/fragmentPersonalNewsDetail"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
        <action
            android:id="@+id/action_destination_news_to_destination_home"
            app:destination="@id/destination_home"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
    </fragment>
    <fragment
        android:id="@+id/destination_fun"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.FragmentFun"
