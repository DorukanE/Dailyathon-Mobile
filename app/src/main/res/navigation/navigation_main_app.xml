        <action
            android:id="@+id/action_destination_dailyathon_to_fragmentCurrency"
            app:destination="@id/fragmentCurrency"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
        <action
            android:id="@+id/action_destination_dailyathon_to_destination_home"
            app:destination="@id/destination_home"
