        tools:layout="@layout/fragment_daily_news_detail" >
        <action
            android:id="@+id/action_fragmentDailyNewsDetail_to_fragmentDailyNews"
            app:destination="@id/fragmentDailyNews"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
        <argument android:name="newsTitle"
            app:argType="string"
            android:defaultValue="default" />
        <argument
            android:name="newsDescription"
            app:argType="string"
            android:defaultValue="default" />
        <argument
            android:name="newsType"
            app:argType="string"
            android:defaultValue="default" />
    </fragment>
    <fragment
        android:id="@+id/fragmentPersonalNewsDetail"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.fragmentsNews.FragmentPersonalNewsDetail"
        android:label="fragment_personal_news_detail"
        tools:layout="@layout/fragment_personal_news_detail" >
        <action
            android:id="@+id/action_fragmentPersonalNewsDetail_to_fragmentPersonalNews"
            app:destination="@id/fragmentPersonalNews"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
        <argument android:name="newsTitle"
            app:argType="string"
            android:defaultValue="default" />
        <argument
            android:name="newsDescription"
            app:argType="string"
            android:defaultValue="default" />
        <argument
            android:name="newsType"
            app:argType="string"
            android:defaultValue="default" />
