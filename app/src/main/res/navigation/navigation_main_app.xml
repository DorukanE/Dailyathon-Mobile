        android:id="@+id/fragmentCurrency"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.fragmentsCurrency.FragmentCurrency"
        android:label="FragmentCurrency" >
        <action
            android:id="@+id/action_fragmentCurrency_to_destination_dailyathon"
            app:destination="@id/destination_dailyathon"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAnnouncement"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.fragmentsAnnouncement.FragmentAnnouncement"
