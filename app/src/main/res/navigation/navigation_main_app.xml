            android:id="@+id/action_destination_news_to_fragmentDailyNewsDetail"
            app:destination="@id/fragmentDailyNewsDetail"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
