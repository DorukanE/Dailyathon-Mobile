            app:exitAnim="@anim/fragment_fade_exit" />
        <argument
            android:name="leagueName"
            app:argType="string" />
        <argument
            android:name="leagueID"
            app:argType="integer" />
        <argument android:name="sportID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/fragmentUserSport"
        android:name="com.dorukaneskiceri.dailyathon.fragmentsMain.fragmentsSport.FragmentUserSport"
        android:label="fragment_user_sport"
        tools:layout="@layout/fragment_user_sport" >
